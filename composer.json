{
    "name": "andcarpi/pt-br-validator",
    "description": "Uma biblioteca contendo validações de formatos Brasileiros, para o Laravel 5.*",
    "license": "MIT",
    "authors": [
        {
            "name": "Anderson Carpi",
            "email": "kustelac@yahoo.com.br"
        }
    ],
    "require": {
        "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.*"
    },
    "autoload": {
        "psr-4": {
            "andcarpi\\PtBrValidator\\": "src"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "orchestra/testbench": "3.1.*",
        "phpunit/phpunit": "4.8.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "andcarpi\\PtBrValidator\\ValidatorProvider"
            ]
        }
    }
}
